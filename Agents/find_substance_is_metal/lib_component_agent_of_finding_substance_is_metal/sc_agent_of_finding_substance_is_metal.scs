sc_agent_of_finding_substance_is_metal
=> nrel_main_idtf:	
	[sc-агент определение является ли вещество металлом] (* <- lang_ru;; *);
	[sc-agent find is substance a metal] (* <- lang_en;; *);
	
<- abstract_sc_agent;

=> nrel_primary_initiation_condition: 
	(sc_event_add_output_arc => question_initiated);
	
=> nrel_initiation_condition_and_result: 
	(..sc_agent_of_finding_substance_is_metal_condition => ..sc_agent_of_finding_substance_is_metal_result);
	
<= nrel_sc_agent_key_sc_elements: 
	{
	question_initiated;
	question;
	action_find_substance_is_metal
	};
	
=> nrel_inclusion: 
	.platform_independent_realization_of_sc_agent_of_finding_substance_is_metal
	(*
	<- platform_independent_abstract_sc_agent;;
	<= nrel_sc_agent_program: 
		{
		agent_of_finding_substance_is_metal
		};;
		
	-> sc_agent_of_finding_substance_is_metal_scp 
		(* <- active_sc_agent;; *);;
	*);;

..sc_agent_of_finding_substance_is_metal_condition
= [*
	action_find_substance_is_metal _-> .._question;;
	question_initiated _-> .._question;;
	question _-> .._question;;
	.._question _-> .._parameter1;;
*];;

..sc_agent_of_finding_substance_is_metal_result
= [*
	action_find_substance_is_metal _-> .._question;;
	question_finished _-> .._question;;
	question _-> .._question;;
	.._question _=> nrel_answer:: .._answer_for_checking_domains_of_relation;;
	.._question _-> .._parameter1;;
*];;
