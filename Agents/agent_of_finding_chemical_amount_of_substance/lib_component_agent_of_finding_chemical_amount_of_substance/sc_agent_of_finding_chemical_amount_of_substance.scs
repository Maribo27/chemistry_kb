sc_agent_of_finding_chemical_amount_of_substance
=> nrel_main_idtf:	
	[sc-агент поиск химического количества вещества] (* <- lang_ru;; *);
	[sc-agent find chemical amount of substance] (* <- lang_en;; *);
	
<- abstract_sc_agent;

=> nrel_primary_initiation_condition: 
	(sc_event_add_output_arc => question_initiated);
	
=> nrel_initiation_condition_and_result: 
	(..sc_agent_of_finding_chemical_amount_of_substance_condition => ..sc_agent_of_finding_chemical_amount_of_substance_result);
	
<= nrel_sc_agent_key_sc_elements: 
	{
	question_initiated;
	question;
	action_find_chemical_amount_of_substance
	};
	
=> nrel_inclusion: 
	.platform_independent_realization_of_sc_agent_of_finding_chemical_amount_of_substance
	(*
	<- platform_independent_abstract_sc_agent;;
	<= nrel_sc_agent_program: 
		{
		agent_of_finding_chemical_amount_of_substance
		};;
		
	-> sc_agent_of_finding_chemical_amount_of_substance_scp 
		(* <- active_sc_agent;; *);;
	*);;

..sc_agent_of_finding_chemical_amount_of_substance_condition
= [*
	action_find_chemical_amount_of_substance _-> .._question;;
	question_initiated _-> .._question;;
	question _-> .._question;;
	.._question _-> .._parameter1;;
*];;

..sc_agent_of_finding_chemical_amount_of_substance_result
= [*
	action_find_chemical_amount_of_substance _-> .._question;;
	question_finished _-> .._question;;
	question _-> .._question;;
	.._question _=> nrel_answer:: .._answer_for_checking_domains_of_relation;;
	.._question _-> .._parameter1;;
*];;